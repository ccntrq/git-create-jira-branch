{
  "name": "git-create-jira-branch",
  "version": "2.0.0",
  "description": "Setup feature branches for your Jira tickets with one command",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ccntrq/git-create-jira-branch.git"
  },
  "bugs": "https://github.com/ccntrq/git-create-jira-branch/issues",
  "author": {
    "name": "Alexander 'ccntrq' Pankoff",
    "email": "ccntrq@screenri.de"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "volta": {
    "node": "20.9.0"
  },
  "keywords": [
    "git",
    "jira",
    "productivity"
  ],
  "bin": {
    "git-create-jira-branch": "dist/main.js"
  },
  "files": [
    "/dist"
  ],
  "scripts": {
    "start": "npm run build && node dist/main.js",
    "build": "tsc --build src",
    "clean": "rimraf dist/ coverage/",
    "prepack": "npm run clean && npm run build",
    "prepare": "husky install",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:type-check": "tsc -p tsconfig.test.json",
    "pretest": "npm run test:type-check",
    "lint": "eslint .",
    "lint:fix": "npm run lint -- --fix"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.4.3",
    "@commitlint/config-conventional": "^18.4.3",
    "@tsconfig/node20": "^20.1.2",
    "@tsconfig/strictest": "^2.0.2",
    "@types/node": "^20.10.3",
    "@typescript-eslint/eslint-plugin": "^6.13.2",
    "@typescript-eslint/parser": "^6.13.2",
    "@typescript-eslint/typescript-estree": "^6.9.0",
    "@vitest/coverage-v8": "^1.0.1",
    "@vitest/ui": "^1.0.1",
    "eslint": "^8.55.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^5.0.1",
    "husky": "^8.0.3",
    "prettier": "^3.1.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.3.3",
    "vitest": "^1.0.1"
  },
  "dependencies": {
    "@effect/cli": "0.24.0",
    "@effect/platform": "0.33.0",
    "@effect/platform-node": "0.34.0",
    "effect": "2.0.0-next.59"
  }
}